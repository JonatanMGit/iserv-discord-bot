{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 16",
  "compilerOptions": {
    "lib": [
      "es2021"
    ],
    "module": "commonjs",
    "target": "es2021",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowUnreachableCode": false,
    "outDir": "./dist",
    "rootDir": "./src",
    "typeRoots": [
      "node_modules/@types"
    ],
    "sourceMap": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "declaration": true,
    "newLine": "LF"
  }
}
